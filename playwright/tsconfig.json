{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "paths": {
      "@elements/*": ["./elements/*"],
      "@engine/*": ["./engine/*"],
      "@pageObjects/*": ["./page_objects/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
